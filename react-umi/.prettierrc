{
	"printWidth": 80,
	"singleQuote": true,
	"trailingComma": "none",
	"semi": false,
	"useTabs": true,
	"proseWrap": "never",
	"overrides": [
		{
			"files": ".prettierrc",
			"options": {
				"parser": "json"
			}
		}
	],
	"plugins": ["prettier-plugin-organize-imports", "prettier-plugin-packagejson"]
}
